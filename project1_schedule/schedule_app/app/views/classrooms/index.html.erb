<h2>Listing classrooms</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Maxpeople</th>
    <th>Course</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Teacher</th>
  </tr>

<% @classrooms.each do |classroom| %>
  <tr>
    <td><%= classroom.name %></td>
    <td><%= classroom.maxpeople %></td>
    
    
    <td><%= link_to 'Show', classroom %></td>

    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Classroom', new_classroom_path %>
